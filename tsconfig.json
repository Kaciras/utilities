{
	"compilerOptions": {
		"target": "ESNext",
		"lib": [
			"ESNext",
			"DOM",
			"DOM.Iterable"
		],
		"outDir": "lib",
		"emitDeclarationOnly": true,
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"declaration": true,
		"strict": true,
		"skipLibCheck": true,
		"useUnknownInCatchVariables": false,
		"allowImportingTsExtensions": true,
	}
}
